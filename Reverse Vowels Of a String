//Reverse Vowels of a string


int main()
{
	char a[100],c[100];
	gets(a);
	int b=strlen(a);
	int i,j=0,k;
	for(i=0;i<b;i++)
	{
		if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u')
		{
			c[j]=a[i];
			j++;
		}
	}
	strrev(c);
	j=0;
	for(i=0;i<b;i++)
	{
		if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u')
		{
			a[i]=c[j];
			j++;
		}
	}
	puts(a);
	
}
