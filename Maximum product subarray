//Maximum Product SubArray


int main()
{
	int i,j,k=0;
	int n,a[100],max[100];
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=0;i<n-1;i++)
	{
		for(j=i+1;j<=i+1;j++)
		{
		   max[k]=	a[i]*a[j];
		   k++;
		}
		
	}
	int d=max[0];
	for(i=0;i<k;i++)
	{
		if(max[i]>d)
		{
			d=max[i];
		}
	}
	printf("%d",d);
	
}
